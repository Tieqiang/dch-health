<?xml version="1.0" encoding="UTF-8"?>
<ehcache updateCheck="false" name="hibernateCache">

    <diskStore path="java.io.tmpdir/jeesite/ehcache/hibernate"/>
    <defaultCache
            maxElementsOnDisk="0"
            eternal="true"
            overflowToDisk="true"
            diskPersistent="false"
            timeToIdleSeconds="0"
            timeToLiveSeconds="0"
            diskSpoolBufferSizeMB="50"
            diskExpiryThreadIntervalSeconds="120"
            memoryStoreEvictionPolicy="LFU"
    />



    <cache name="com.jims.his.modules.sys.entity.Dict"  eternal="false" overflowToDisk="true"
    />

    <cache name="com.jims.his.modules.sys.entity.Area"  eternal="false" overflowToDisk="true"
    />
    <cache name="com.jims.his.modules.sys.entity.Area.childList"  eternal="false"
           overflowToDisk="true"/>
    <cache name="com.jims.his.modules.sys.entity.Area.officeList"  eternal="false"
           overflowToDisk="true"/>

    <cache name="com.jims.his.modules.sys.entity.Office"  eternal="false" overflowToDisk="true"
    />
    <cache name="com.jims.his.modules.sys.entity.Office.childList"  eternal="false"
           overflowToDisk="true"/>
    <cache name="com.jims.his.modules.sys.entity.Office.userList"  eternal="false"
           overflowToDisk="true"/>

    <cache name="com.jims.his.modules.sys.entity.Menu"  eternal="false" overflowToDisk="true"
    />
    <cache name="com.jims.his.modules.sys.entity.Menu.childList"  eternal="false"
           overflowToDisk="true"/>
    <cache name="com.jims.his.modules.sys.entity.Menu.roleList"  eternal="false"
           overflowToDisk="true"/>

    <cache name="com.jims.his.modules.sys.entity.Role"  eternal="false" overflowToDisk="true"
    />
    <cache name="com.jims.his.modules.sys.entity.Role.menuList"  eternal="false"
           overflowToDisk="true"/>
    <cache name="com.jims.his.modules.sys.entity.Role.userList"  eternal="false"
           overflowToDisk="true"/>

    <cache name="com.jims.his.modules.sys.entity.User"  eternal="false" overflowToDisk="true"
    />
    <cache name="com.jims.his.modules.sys.entity.User.roleList"  eternal="false"
           overflowToDisk="true"/>

</ehcache>
