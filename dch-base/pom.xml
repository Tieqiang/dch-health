    <parent>
        <groupId>com.dch</groupId>
        <artifactId>dch-health</artifactId>
        <version>1.0-SNAPSHOT</version>
    </parent>
    <modelVersion>4.0.0</modelVersion>

    <artifactId>dch-base</artifactId>
    <build>
        <plugins>
            <plugin>
